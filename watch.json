{
    "to_watch": {
        "root": [
            "src/"
        ],
        "format": [
            ".rs"
        ],
        "commands":[
            "cargo build",
            "echo Compilation finished for !{$}!"
        ]
    },
    "key_events": [
        {
            "keys": ["LAlt", "LShift"],
            "commands": [
                "start cargo run"
            ]
        }
    ]
}